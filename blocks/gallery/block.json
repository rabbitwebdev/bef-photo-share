{
    "name": "acf/gallery",
    "title": "Gallery",
    "description": "A BEF Photo gallery block that uses ACF fields.",
    "style": [ "file:./gallery.css" ],
    "category": "formatting",
    "icon": "admin-comments",
    "keywords": ["gallery", "quote"],
    "acf": {
        "mode": "preview",
        "renderTemplate": "gallery.php"
    },
    "supports": {
        "anchor": true,
         "align": true,
        "html": true,
        "jsx": true,
        "mode": true,
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": "var:preset|spacing|50"
        },
        "color": {
            "background": true,
            "text": true
        }
    }
}